    "prettier": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\"",
    "prettier:fix": "npm run prettier -- --write",
    "release:changelog": "standard-version"
  },
  "dependencies": {
    "pinia": "^2.0.23",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
