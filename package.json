{
  "name": "way-sight",
  "version": "0.0.1",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
